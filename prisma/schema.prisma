datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Image {
  id          Int       @id @default(autoincrement())
  discordId   String  
  url         String
  fileName    String
  createdAt   DateTime  @default(now())
  description String?
  height      Int
  width       Int
  size        Int
}