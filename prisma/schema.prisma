datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Image {
  id          String    @id @unique
  discordId   String  
  url         String
  fileName    String
  createdAt   DateTime  @default(now())
  description String?
  height      Int
  width       Int
  size        Int
}